{
  "dxt_version": "0.1.0",
  "name": "google-apps-script-mcp",
  "version": "1.1.0",
  "description": "Google Apps Script MCP server with Clasp Integration",
  "author": {
    "name": "Utakata",
    "email": "contact@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Utakata/google-apps-script-mcp.git"
  },
  "server": {
    "command": "node",
    "args": [
      "src/index-clasp-integrated.js"
